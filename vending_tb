S_0116E200 .scope module, "uut" "vending_machine" 2 11, 3 1, S_0116E310;
 .timescale 0 0;
P_011696B4 .param/l "S0" 3 11, C4<00>;
P_011696C8 .param/l "S10" 3 11, C4<10>;
