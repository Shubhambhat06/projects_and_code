v01163A20_0 .net "reset", 0 0, v0116B7F0_0; 1 drivers
v01163278_0 .var "state", 1 0;
E_01168910 .event edge, v01163278_0, v01169838_0;
E_01168870 .event posedge, v01163A20_0, v011697E0_0;
    .scope S_0116E200;
T_0 ;
    %wait E_01168870;
    %load/v 8, v01163A20_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 2, 0;
    %assign/v0 v01163278_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v011639C8_0, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v01163278_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0116E200;
T_1 ;
    %wait E_01168910;
    %set/v v0116A690_0, 0, 1;
    %load/v 8, v01163278_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.2, 6;
    %set/v v011639C8_0, 0, 2;
    %set/v v0116A690_0, 0, 1;
    %jmp T_1.4;
T_1.0 ;
    %load/v 8, v01169838_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_1.5, 4;
    %movi 8, 1, 2;
    %set/v v011639C8_0, 8, 2;
    %jmp T_1.6;
T_1.5 ;
    %load/v 8, v01169838_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_1.7, 4;
    %movi 8, 2, 2;
    %set/v v011639C8_0, 8, 2;
    %jmp T_1.8;
T_1.7 ;
    %set/v v011639C8_0, 0, 2;
T_1.8 ;
T_1.6 ;
    %jmp T_1.4;
T_1.1 ;
    %load/v 8, v01169838_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_1.9, 4;
    %movi 8, 2, 2;
    %set/v v011639C8_0, 8, 2;
    %jmp T_1.10;
T_1.9 ;
    %load/v 8, v01169838_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_1.11, 4;
    %set/v v011639C8_0, 0, 2;
    %set/v v0116A690_0, 1, 1;
    %jmp T_1.12;
T_1.11 ;
    %movi 8, 1, 2;
    %set/v v011639C8_0, 8, 2;
T_1.12 ;
T_1.10 ;
    %jmp T_1.4;
T_1.2 ;
    %load/v 8, v01169838_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_1.13, 4;
    %set/v v011639C8_0, 0, 2;
    %set/v v0116A690_0, 1, 1;
    %jmp T_1.14;
T_1.13 ;
    %load/v 8, v01169838_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_1.15, 4;
    %set/v v011639C8_0, 0, 2;
    %set/v v0116A690_0, 1, 1;
