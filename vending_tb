T_1.15 ;
    %movi 8, 2, 2;
    %set/v v011639C8_0, 8, 2;
T_1.16 ;
T_1.14 ;
    %jmp T_1.4;
T_1.4 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0116E310;
T_2 ;
    %set/v v011632D0_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/v 8, v011632D0_0, 1;
    %inv 8, 1;
    %set/v v011632D0_0, 8, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0116E310;
T_3 ;
    %vpi_func 2 30 "$fopen", 8, 32, "coins_input.txt", "r";
    %set/v v011A2BD0_0, 8, 32;
    %load/v 8, v011A2BD0_0, 32;
    %cmpi/u 8, 0, 32;
    %jmp/0xz  T_3.0, 4;
    %vpi_call 2 32 "$display", "ERROR: Could not open coins_input.txt";
    %vpi_call 2 33 "$finish";
T_3.0 ;
    %set/v v0116B7F0_0, 1, 1;
    %set/v v01163328_0, 0, 2;
    %delay 10000, 0;
    %set/v v0116B7F0_0, 0, 1;
T_3.2 ;
    %vpi_func 2 41 "$feof", 8, 32, v011A2BD0_0;
    %nor/r 8, 8, 32;
    %jmp/0xz T_3.3, 8;
    %vpi_func 2 42 "$fscanf", 8, 32, v011A2BD0_0, "%d\012", v011A2B20_0;
    %set/v v0116B848_0, 8, 32;
    %load/v 8, v0116B848_0, 32;
    %cmpi/u 8, 1, 32;
    %jmp/0xz  T_3.4, 4;
    %load/v 8, v011A2B20_0, 32;
    %cmpi/u 8, 5, 32;
    %jmp/0xz  T_3.6, 4;
    %movi 8, 1, 2;
    %set/v v01163328_0, 8, 2;
    %jmp T_3.7;
T_3.6 ;
    %load/v 8, v011A2B20_0, 32;
