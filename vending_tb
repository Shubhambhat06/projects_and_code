S_0116E310 .scope module, "tb" "tb" 2 3;
 .timescale -9 -12;
v011632D0_0 .var "clk", 0 0;
v01163328_0 .var "coin", 1 0;
v011A2B20_0 .var/i "cval", 31 0;
v011A2B78_0 .net "dispense", 0 0, v0116A690_0; 1 drivers
